<mvc:View 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="de.integrata.training.16961.lesson.5extd.controller.Master">
	<Page title="{i18n>CustomerPageTitle}">
		<content>
			<List items="{dataModel>/BapiscudatSet}">
				<items>
					<StandardListItem 
						title="{dataModel>Custname}"
						info="{dataModel>City}"
						type="Active"
						press="toCustomerDetail"
					/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>