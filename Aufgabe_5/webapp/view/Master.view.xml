<mvc:View 
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="de.integrata.training.16961.lesson.5.controller.Master">
	<Page title="{i18n>CustomerPageTitle}">
		<content>
			<List items="{dataModel>/BapiscudatSet}" itemPress="toCustomerDetail">
				<items>
					<StandardListItem 
						title="{dataModel>Custname}"
						info="{dataModel>City}"
						type="Active"
					/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>